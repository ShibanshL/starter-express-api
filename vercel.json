{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "method":["GET","POST","PATCH","PUT","DELETE","OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin": "*"
        }
      }
    ]
}